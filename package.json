{
  "name": "com.angga.fizzbuzz",
  "version": "1.0.0",
  "description": "FizzBuzz Program using NodeJS & Typescript",
  "main": "src/app.ts",
  "scripts": {
    "dev": "ts-node src/app.ts",
    "start": "ts-node build/src/app.js",
    "build": "tsc -p .",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register",
    "test:all": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "ts-coverage": "typescript-coverage-report"
  },
  "author": "muh.angga.1998",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@types/yargs": "^17.0.22",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-coverage-report": "^0.7.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "winston": "^3.8.2",
    "yargs": "^17.6.2"
  }
}
